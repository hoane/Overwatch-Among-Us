void spawnBots(ref define botsToCreate) {
    define botHeros = ChooseArray(ALLOWED_HEROS, botsToCreate)
    for (define i = 0; i < botsToCreate; i++) {
        CreateDummyBot(botHeros[i], Team.All, -1, Vector(-190, 1 + i * 2, -30));
    }
}